{
  "name": "travelflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.18.1",
  "scripts": {
    "dev": "vite",
    "dev:netlify": "npx netlify dev --port 8888 --target-port 5173",
    "i18n:validate": "node scripts/validate-i18n.mjs",
    "updates:validate": "node scripts/validate-updates.mjs",
    "plans:validate-sync": "tsx scripts/validate-plan-sync.mjs",
    "plans:generate-seed": "tsx scripts/generate-plan-seed.mjs",
    "updates:next-version": "node scripts/next-release-version.mjs",
    "blog:validate": "node scripts/validate-blog.mjs",
    "edge:validate": "node scripts/validate-edge-functions.mjs",
    "sitemap:generate": "node scripts/generate-sitemap.mjs",
    "inspirations:images:jobs": "tsx scripts/build-inspiration-image-batch.ts",
    "build:images": "tsx scripts/build-inspiration-images.ts",
    "inspirations:images:optimize": "tsx scripts/build-inspiration-images.ts --skip-generation",
    "blog:images:jobs": "tsx scripts/build-blog-image-batch.ts",
    "build:blog-images": "tsx scripts/build-blog-images.ts",
    "blog:images:optimize": "tsx scripts/build-blog-images.ts --skip-generation",
    "build:image-placeholders": "tsx scripts/build-image-placeholders.ts",
    "release:prepare": "pnpm build:blog-images && pnpm build",
    "build": "pnpm inspirations:images:optimize && pnpm blog:images:optimize && pnpm build:image-placeholders && pnpm i18n:validate && pnpm plans:validate-sync && pnpm updates:validate && pnpm blog:validate && pnpm edge:validate && pnpm sitemap:generate && vite build",
    "preview": "vite preview",
    "maps:generate": "tsx scripts/generate-trip-maps.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@google/genai": "latest",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@supabase/supabase-js": "^2.49.1",
    "@types/google.maps": "^3.58.1",
    "blurhash": "^2.0.5",
    "flagpack": "^1.0.5",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-icu": "^2.4.1",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "0.378.0",
    "lz-string": "1.5.0",
    "prismjs": "^1.30.0",
    "react": "18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "18.3.1",
    "react-i18next": "^16.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.182.0",
    "vaul": "^0.9.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "jsdom": "^28.1.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.18",
    "terser": "^5.46.0",
    "tsx": "^4.19.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.18"
  }
}

{
  "name": "travelflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "updates:validate": "node scripts/validate-updates.mjs",
    "blog:validate": "node scripts/validate-blog.mjs",
    "edge:validate": "node scripts/validate-edge-functions.mjs",
    "inspirations:images:jobs": "tsx scripts/build-inspiration-image-batch.ts",
    "build:images": "tsx scripts/build-inspiration-images.ts",
    "inspirations:images:optimize": "tsx scripts/build-inspiration-images.ts --skip-generation",
    "blog:images:jobs": "tsx scripts/build-blog-image-batch.ts",
    "build:blog-images": "tsx scripts/build-blog-images.ts",
    "blog:images:optimize": "tsx scripts/build-blog-images.ts --skip-generation",
    "release:prepare": "npm run build:blog-images && npm run build",
    "build": "npm run inspirations:images:optimize && npm run blog:images:optimize && npm run updates:validate && npm run blog:validate && npm run edge:validate && vite build",
    "preview": "vite preview",
    "maps:generate": "tsx scripts/generate-trip-maps.ts"
  },
  "dependencies": {
    "@google/genai": "latest",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@supabase/supabase-js": "^2.49.1",
    "@types/google.maps": "^3.58.1",
    "lucide-react": "0.378.0",
    "lz-string": "1.5.0",
    "react": "18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.182.0",
    "vaul": "^0.9.9"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.19.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
